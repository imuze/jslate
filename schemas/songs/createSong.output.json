{
    "title": "createSongOutput",
    "description": "This endpoint create a song.",
    "method": "POST",
    "route": "/songs",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "minimum": 0
        },
        "title": {
            "type": "string"
        },
        "musical_mode": {
            "enum": ["Ionian", "Dorian", "Phrygian", "Lydian", "Mixolydia", "Aeolian", "Locrian"]
        },
        "arrangement_id": {
            "oneOf": [{
                "type": "integer",
                "minimum": 0
            }, {
                "type": "null"
            }]
        },
        "timeline_url": {
            "type": "string"
        },
        "mp3": {
            "type": "string"
        },
        "status_channel": {
            "type": "string"
        }
    },
    "required": ["title", "musical_mode", "arrangement_id", "timeline_url", "mp3", "status_channel"]
}
